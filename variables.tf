# Subscription ID
variable "subscriptionid" {
    default = "your subscription id"
}

variable "tenantid" {
    default = "your tenant id"
}

# Ressource Group Name
variable "rg" {
    default = "DynDNS"
}

# Azure Region
variable "region" {
    default = "West Europe"
}

# AzureDNS Domain Name
variable "domainname" {
    default = "dyndnstest.com"
}

# AzureDNS A Record
variable "arecord" {
    default = "home"
}

